<Notice
  class="topology-limited-access-banner"
  ...attributes
  @type={{or @type "info"}}
as |notice|>
    <notice.Header>
    <h3>
      Limited Access
    </h3>
  </notice.Header>
  <notice.Body>
    <p>
      This service may have dependencies you won’t see because you don’t have access to them.
    </p>
  </notice.Body>
</Notice>