<div
  class={{class-map
    'consul-server-list'
  }}
  ...attributes
>
  <ul>
{{#each @items as |item|}}
    <li>
      <a href={{href-to 'dc.nodes.show' item.Name}}>
        <Consul::Server::Card
          @item={{item}}
        />
      </a>
    </li>
{{/each}}
  </ul>
</div>

