{{#if this.itemsToDisplay.length}}
  <dl class="consul-bucket-list">
    {{#each this.itemsToDisplay as |item|}}
      <dt class={{item.type}} {{tooltip}}>
        {{item.label}}
      </dt>
      <dd data-test-bucket-item={{item.type}}>
        {{item.item}}
      </dd>
    {{/each}}
  </dl>
{{/if}}
