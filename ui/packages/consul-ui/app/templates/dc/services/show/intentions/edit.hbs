<Route
  @name={{routeName}}
as |route|>
  <Consul::Intention::Form
    @nspace={{'*'}}
    @dc={{route.params.dc}}
    @partition={{route.params.partition}}
    @src={{route.params.intention_id}}
    @autofill={{hash
      DestinationName=route.params.name
    }}
    @onsubmit={{transition-to 'dc.services.show.intentions.index'}}
  />
</Route>
