<div
  ...attributes
>
  <Hds::ButtonSet>
    <Hds::Button
      @text='Copy token'
      @color='primary'
      ...attributes
      {{with-copyable @token}}
    />
    <Hds::Button
      @text='Close'
      type='reset'
      @color='secondary'
      {{on 'click' @onclose}}
    />
  </Hds::ButtonSet>
</div>
