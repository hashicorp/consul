<div id="meta" class="tab-section">
  <div role="tabpanel">
    <section class="tags">
      <h3>Tags</h3>
    {{#if (gt item.Tags.length 0) }}
      <TagList @item={{item}} />
    {{else}}
      <p>
        There are no tags.
      </p>
    {{/if}}
    </section>
    <section class="metadata">
      <h3>Meta</h3>
    {{#if item.Meta}}
      <ConsulMetadataList @items={{object-entries item.Meta}} />
    {{else}}
      <p>
        This instance has no metadata.
      </p>
    {{/if}}
    </section>
  </div>
</div>
