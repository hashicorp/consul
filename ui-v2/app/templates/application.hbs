{{#if (not loading)}}
  {{outlet}}
{{else}}
  {{#hashicorp-consul id="wrapper" permissions=permissions dcs=dcs dc=dc nspaces=nspaces nspace=nspace}}
    {{#app-view class="loading show"}}
      {{#block-slot 'content'}}
        {{partial 'consul-loading'}}
      {{/block-slot}}
    {{/app-view}}
  {{/hashicorp-consul}}
{{/if}}
