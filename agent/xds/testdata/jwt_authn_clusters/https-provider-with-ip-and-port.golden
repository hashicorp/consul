{
  "loadAssignment": {
    "clusterName": "jwks_cluster_okta",
    "endpoints": [
      {
        "lbEndpoints": [
          {
            "endpoint": {
              "address": {
                "socketAddress": {
                  "address": "127.0.0.1",
                  "portValue": 9091
                }
              }
            }
          }
        ]
      }
    ]
  },
  "name": "jwks_cluster_okta",
  "transportSocket": {
    "name": "tls",
    "typedConfig": {
      "@type":"type.googleapis.com/envoy.extensions.transport_sockets.tls.v3.UpstreamTlsContext",
      "commonTlsContext": {
        "validationContext":  {
          "trustedCa": {
            "filename": "mycert.crt"
          }
        }
      }
    }
  },
  "connectTimeout": "5s",
  "type": "STATIC"
}