# consul-agent -- The consul agent daemon 
#
# Configure consul-agent in /etc/consul/conf.d/

start on runlevel [2345]

respawn

setuid consul
setgid consul

exec /usr/bin/consul agent -config-file=/etc/consul/consul-agent.hcl -config-dir=/etc/consul/conf.d/ -data-dir=/var/cache/consul/ >> /var/log/consul.log 2>&1
