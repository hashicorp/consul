<configuration>
  <!-- This makes winsw work with both .NET 2.0 and .NET 4.x -->
  <startup>
    <supportedRuntime version="v2.0.50727" />
    <supportedRuntime version="v4.0" />
  </startup>
  <!-- This makes Windows disable AuthentiCode checks, which would otherwise require an internet connection -->
  <runtime>
    <generatePublisherEvidence enabled="false"/> 
  </runtime>
</configuration>